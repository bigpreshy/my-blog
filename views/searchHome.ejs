<div class="author">
    <h1 class="author__heading">Search Results</h1>
  </div>
  
  <section class="articles">
    <!-- console.log()s -->
  <% if (posts.length > 0) { %>
    <ul class="article-ul">
      <% posts.forEach(post => { %>
        <li>
          <a href="/article/<%= post.slug %>">
            <span><%= post.title %></span>
            <span class="article-list__date"><%= post.createdAt.toDateString() %></span>
          </a>
        </li>
      <% }) %>
    </ul>
   
  <% } %>

  <h3>Nothing found by this keyword "<%= searchTerm %>"</h3>
   
  
  
  </section>